{
  "name": "script-generator",
  "productName": "script-generator",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/main.js",
  "scripts": {
    "start": "ng build && electron-forge start",
    "package": "ng build --prod=true && electron-forge package --platform=win32 --arch=x64",
    "make": "electron-forge make --platform=win32 --arch=x64 --skip-package",
    "packmake": "ng build --prod=true && electron-forge make --platform=win32 --arch=x64",
    "publish": "electron-forge publish",
    "lint": "eslint src --color",
    "elec": "ng build && electron .",
    "elec-start": "electron-forge start --inspect-electron",
    "start-node": "node bin/www",
    "debug-node": "nodemon --inspect bin/www"
  },
  "keywords": [],
  "author": "Manikandan.R",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "packageManager": "npm",
        "out": "release-build",
        "asar": true,
        "overwrite": true,
        "icon": "src/assets/icons/win/icon.ico",
        "prune": true,
        "ignore": [
          ".gitignore",
          ".idea",
          "README.md"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "platforms": [
            "win32"
          ],
          "config": {
            "name": "script-generator",
            "loadingGif": "src/assets/images/loading.gif"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "win32",
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "platforms": [
            "linux"
          ],
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "platforms": [
            "linux"
          ],
          "config": {}
        }
      ],
      "publishers": [
        {
          "github_repository": {
            "owner": "mani0608",
            "name": "script-generator"
          }
        }
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/cdk": "^7.0.4",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.0.4",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@types/file-saver": "^2.0.0",
    "@types/jszip": "^3.1.4",
    "@types/lodash": "^4.14.118",
    "@types/pouchdb": "^6.3.2",
    "@types/quill": "^2.0.1",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "collections": "^5.1.5",
    "core-js": "^2.5.7",
    "cors": "^2.8.5",
    "dnode": "^1.2.2",
    "express": "^4.16.4",
    "express-fileupload": "^1.0.0",
    "file-saver": "^2.0.0",
    "filereader": "^0.10.3",
    "font-awesome": "^4.7.0",
    "highlightjs": "^9.12.0",
    "jquery": "^3.3.1",
    "jszip": "^3.1.5",
    "lodash": "^4.17.11",
    "memory-cache": "^0.2.0",
    "ngx-dropzone-wrapper": "^7.0.0",
    "ngx-quill": "^4.1.0",
    "popper.js": "^1.14.5",
    "pouchdb": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "quill": "^1.3.6",
    "rxjs": "~6.3.3",
    "serve-favicon": "^2.5.0",
    "sqlformatter": "^0.1.0",
    "squel": "^5.12.2",
    "stream": "0.0.2",
    "winston": "^3.1.0",
    "xlsx": "^0.14.1",
    "xlsx-style": "^0.8.13",
    "zone.js": "~0.8.26"
  },
